/* Check if dirtyinfnorm seems to return a faithful rounding of the true value ||f||_I */
test_dirtyinfnorm = proc(f, I) {
  var oldprec, res, infr, supr, L, L2, t, r, stop, a, nbiter;
  oldprec = prec;
  prec = prec + 100!;
  L = dirtyfindzeros(diff(f), I);
  L2 = [||];
  for t in L do {
    L2 = L2 :. [round(t, oldprec, RD), round(t, oldprec, RU)];
  };
  L = [inf(I),inf(I)].:L2:.[sup(I),sup(I)];

  stop = false;
  nbiter = 0;
  while !stop do {
    infr = 0; supr = 0;
    for t in L do {
      r = evaluate(abs(f), t);
      if inf(r) > infr then infr = inf(r);
      if sup(r) > supr then supr = sup(r);
    };
    r = [infr, supr];
    a := floor(-log2( (sup(r)-inf(r)) / inf(abs(r)) ));
    if a >= oldprec+10 then stop = true;
    nbiter = nbiter + 1;
    prec = prec + 100!;
    if nbiter >= 5 then { nbiter = @Inf@; stop = true; };
  };
  if nbiter == @Inf@ then print("test_dirtyinfnorm did not manage to evaluate f with enough accuracy. The result of the test might be unreliable.");

  prec = oldprec!;
  res=dirtyinfnorm(f,I);
  if res in [round(inf(r), oldprec, RD), round(sup(r), oldprec, RU)]
  then print("dirtyinfnorm(", f, ",", I,") at precision", oldprec, "returns a faithful result")
  else print("dirtyinfnorm(", f, ",", I,") at precision", oldprec, "returned", res, "which is neither", round(inf(r), oldprec, RD), "nor", round(sup(r), oldprec, RU),". The result is hence not faithful.");
};

